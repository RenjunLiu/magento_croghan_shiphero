{
    "name": "croghan/magento_croghan_shiphero",
    "type": "magento-module",
    "description": "CatalogInventory feature extension for Magento using ShipHero (shiphero.com)",
    "homepage": "https://github.com/croghan/magento_croghan_shiphero",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Michael Croghan",
            "email": "mike@digitalkeg.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "magento-hackathon/magento-composer-installer": "*"
    },
    "require-dev": {
        "ecomdev/ecomdev_phpunit": "^0.3.7",
        "phpunit/php-invoker": "~1.1"
    },
    "suggest": {},
    "extra": {
        "magento_connect": {
            "channel": "community",
            "php_min": "5.3.0",
            "php_max": "6.0.0",
            "stability": "beta",
            "license_uri": "http://opensource.org/licenses/osl-3.0.php",
            "summary": "CatalogInventory feature extension for Magento",
            "content": [
                {
                    "type": "magecommunity",
                    "structure": "dir",
                    "path": "Croghan/ShipHero"
                },
                {
                    "type": "mageetc",
                    "structure": "file",
                    "path": "modules/Croghan_ShipHero.xml"
                }
            ]
        },
        "map" : [
          ["app/etc/modules/Croghan_ShipHero.xml", "app/etc/modules/Croghan_ShipHero.xml"],
          ["app/code/community/Croghan/ShipHero", "app/code/community/Croghan/ShipHero"]
        ]
    }
}
